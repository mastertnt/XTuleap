<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <OutputType>Library</OutputType>
    <Description>A library to have access to Tuleap (www.Tuleap.org) from .NET</Description>
    <PackageIcon>Tuleap_Icon.png</PackageIcon>
    <PackageProjectUrl>https://github.com/mastertnt/XTuleap</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mastertnt/XTuleap</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.4.0.0</AssemblyVersion>
    <FileVersion>1.4.0.0</FileVersion>
    <Version>1.7.3</Version>
    <PackageReleaseNotes>1.3.0 : Change version to .net standard
1.4.0 : Add an event when an artifact has been retrieved on request
1.7.0 : Change List to ObservableCollection on tracker
1.7.1 : Add INotifyPropertyChanged on Tracker and Artifact</PackageReleaseNotes>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="NLog" Version="5.2.2" />
    <PackageReference Include="PropertyChanged.Fody" Version="4.1.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Tuleap_Icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
